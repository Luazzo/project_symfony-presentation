{% extends 'base.html.twig' %}

{% block title %}Message{% endblock %}

{% block body %}
    <h1>Message</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ message.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ message.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ message.email }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ message.tel }}</td>
            </tr>
            <tr>
                <th>Gsm</th>
                <td>{{ message.gsm }}</td>
            </tr>
            <tr>
                <th>Objet</th>
                <td>{{ message.objet }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ message.slug }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ message.contenu }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ message.sexe }}</td>
            </tr>
            <tr>
                <th>File</th>
                <td>{{ message.file }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ message.path }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('message_index') }}">back to list</a>

    <a href="{{ path('message_edit', {'id': message.id}) }}">edit</a>

    {{ include('message/_delete_form.html.twig') }}
{% endblock %}
